Cls CoffeeMaker {
  init [coffee] {
    this.coffee = coffee;
  }

  brew [] {
    Print "Enjoy your cup of " + this.coffee + "!";
  }
}

Var maker = CoffeeMaker("coffee and chicory");
maker.brew();

# inheritance
Cls CappuchinoMaker < CoffeeMaker {
  init [] {
    this.coffee = "cappuchino";
  }
}

CappuchinoMaker().brew();